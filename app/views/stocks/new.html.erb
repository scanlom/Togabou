<%= form_for :stock, url: stocks_path do |f| %>
  <p>
    <%= f.label :symbol %><br>
    <%= f.text_field :symbol %>
  </p>
  <p>
    <%= f.label :eps %><br>
    <%= f.text_field :eps %>
  </p>
  <p>
    <%= f.label :div %><br>
    <%= f.text_field :div %>
  </p>
  <p>
    <%= f.label :growth %><br>
    <%= f.text_field :growth %>
  </p>
  <p>
    <%= f.label :pe_terminal %><br>
    <%= f.text_field :pe_terminal %>
  </p>
  <p>
    <%= f.label :payout %><br>
    <%= f.text_field :payout %>
  </p>
  <p>
    <%= f.label :book %><br>
    <%= f.text_field :book %>
  </p>
  <p>
    <%= f.label :roe %><br>
    <%= f.text_field :roe %>
  </p>
  <p>
    <%= f.label :model %><br>
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', stocks_path %>
